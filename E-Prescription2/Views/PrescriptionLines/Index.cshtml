@model IEnumerable<E_Prescription2.Models.PrescriptionLine>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.medicationActive)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.dispenseDetails)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.medicationActive.MediActiveId)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.dispenseDetails.DispenseId)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.PrescriptionLineId">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.PrescriptionLineId">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.PrescriptionLineId">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
